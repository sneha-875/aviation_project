{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "devadfoneet"
		},
		"LS_sourceadls_todevadls_output_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_sourceadls_todevadls_output'"
		},
		"LS_REST_API_TO_DEVADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlabs.co/api/v9/airlines"
		},
		"LS_azure_sql_to_devadls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "serverdevv.database.windows.net"
		},
		"LS_azure_sql_to_devadls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sourcefiledb"
		},
		"LS_azure_sql_to_devadls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminmaya"
		},
		"LS_sourceadls_to_devadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sourceadlsonee.dfs.core.windows.net/"
		},
		"LS_sourceadls_to_devadls_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "18bb4a2f-3656-4408-ba29-8e020b096e78"
		},
		"LS_sourceadls_to_devadls_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "3b34f523-9a42-4008-8d67-bd58703bccd8"
		},
		"LS_sourceadls_todevadls_output_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakedevv.dfs.core.windows.net/"
		},
		"dev_AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://my-vaulttq.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "devadfoneet_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_sourceadls_todevadls_output_accountKey": {
						"value": "[parameters('LS_sourceadls_todevadls_output_accountKey')]"
					},
					"LS_REST_API_TO_DEVADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_REST_API_TO_DEVADLS_properties_typeProperties_url')]"
					},
					"LS_sourceadls_todevadls_output_properties_typeProperties_url": {
						"value": "[parameters('LS_sourceadls_todevadls_output_properties_typeProperties_url')]"
					},
					"dev_AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('dev_AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"LS_azure_sql_to_devadls_properties_typeProperties_server": {
						"value": "[parameters('LS_azure_sql_to_devadls_properties_typeProperties_server')]"
					},
					"LS_azure_sql_to_devadls_properties_typeProperties_database": {
						"value": "[parameters('LS_azure_sql_to_devadls_properties_typeProperties_database')]"
					},
					"LS_azure_sql_to_devadls_properties_typeProperties_userName": {
						"value": "[parameters('LS_azure_sql_to_devadls_properties_typeProperties_userName')]"
					},
					"LS_sourceadls_to_devadls_properties_typeProperties_url": {
						"value": "[parameters('LS_sourceadls_to_devadls_properties_typeProperties_url')]"
					},
					"LS_sourceadls_to_devadls_properties_typeProperties_tenant": {
						"value": "[parameters('LS_sourceadls_to_devadls_properties_typeProperties_tenant')]"
					},
					"LS_sourceadls_to_devadls_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('LS_sourceadls_to_devadls_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}